{
  "name": "@logux/backend-test",
  "version": "0.0.0",
  "description": "Test Logux Back-end Protocol implementation",
  "keywords": [
    "logux",
    "server",
    "test"
  ],
  "scripts": {
    "test": "eslint . && yaspeller *.md"
  },
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "homepage": "https://logux.io/",
  "repository": "logux/backend-test",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": "./bin.js",
  "dependencies": {
    "@logux/server": "logux/server#next",
    "chalk": "^4.0.0",
    "ora": "^4.0.4"
  },
  "devDependencies": {
    "@logux/eslint-config": "^39.0.3",
    "@logux/sharec-config": "^0.7.3",
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-let": "^1.0.1",
    "eslint-plugin-prettierx": "^0.12.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^20.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "size-limit": "^4.5.0",
    "yaspeller": "^7.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": "yaspeller",
    "*.js": "eslint --fix"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "eslintConfig": {
    "extends": "@logux/eslint-config"
  },
  "yaspeller": {
    "lang": "en",
    "ignoreCapitalization": true,
    "ignoreText": [
      " \\(by [^)]+\\)."
    ],
    "dictionary": [
      "Versioning",
      "Logux",
      "GraphQL",
      "roadmap"
    ]
  },
  "sharec": {
    "config": "@logux/sharec-config",
    "version": "0.7.3"
  }
}
